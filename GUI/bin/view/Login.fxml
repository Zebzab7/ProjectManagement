<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Button layoutX="142.0" layoutY="279.0" mnemonicParsing="false" onAction="#Login" prefHeight="39.0" prefWidth="116.0" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TextField fx:id="txtUsername" layoutX="88.0" layoutY="138.0" promptText="username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="88.0" layoutY="207.0" promptText="password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblStatus" alignment="CENTER" layoutX="28.0" layoutY="47.0" prefHeight="27.0" prefWidth="344.0" text="Login" textFill="#dd6969">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
