<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.WorkerController">
   <children>
      <Label fx:id="lblWorkerID" layoutX="14.0" layoutY="14.0" text="Welcome back #" />
      <ListView fx:id="listView1" layoutX="15.0" layoutY="77.0" prefHeight="220.0" prefWidth="233.0" />
      <Label layoutX="14.0" layoutY="43.0" text="Projects" />
      <Label layoutX="278.0" layoutY="43.0" text="Users" />
      <ListView fx:id="listView2" layoutX="278.0" layoutY="77.0" prefHeight="220.0" prefWidth="233.0" />
      <Label layoutX="15.0" layoutY="313.0" text="Add a new project" />
      <TextField fx:id="textField1" layoutX="15.0" layoutY="342.0" prefHeight="31.0" prefWidth="233.0" promptText="Name of project" />
      <Button layoutX="15.0" layoutY="381.0" mnemonicParsing="false" onAction="#addProject" prefHeight="31.0" prefWidth="103.0" text="Add project" />
      <Button layoutX="125.0" layoutY="381.0" mnemonicParsing="false" onAction="#removeProject" prefHeight="31.0" prefWidth="121.0" text="Delete project" />
      <Button layoutX="279.0" layoutY="381.0" mnemonicParsing="false" onAction="#AddUserToProject" text="Add user to project" />
      <Label fx:id="lblUser" layoutX="278.0" layoutY="313.0" />
      <Label fx:id="lblProject" layoutX="279.0" layoutY="340.0" />
      <Button layoutX="15.0" layoutY="422.0" mnemonicParsing="false" onAction="#ViewProject" text="View selected project" />
      <Label fx:id="lblStatus" alignment="TOP_LEFT" layoutX="15.0" layoutY="459.0" textFill="#dd6969" />
      <Button layoutX="278.0" layoutY="422.0" mnemonicParsing="false" onAction="#SetAsProjectLeader" text="Set as project leader" />
      <Button layoutX="451.0" layoutY="9.0" mnemonicParsing="false" onAction="#LogOut" text="Log out" />
   </children>
</AnchorPane>
